        id: "{{ user_id }}",                 // email address or other unique identifier of the user
        firstName: "{{ user_firstName }}",
        lastName: "{{ user_lastName }}",
{% if userProps %}
{% for key, value in userProps %}
{{ key }}: "{{ value }}",
{% endfor %}
{% endif %}
